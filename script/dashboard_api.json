{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 1,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "iteration": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "stat",
      "title": "Prediction Requests Rate (req/s)",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(rate(prediction_requests_total{job=~\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "req/s",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" } ] }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Average Prediction Latency (ms)",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "1000 * sum(rate(prediction_latency_ms_sum{job=~\"$job\", instance=~\"$instance\"}[5m])) / sum(rate(prediction_latency_ms_count{job=~\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "avg p50 approx",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 6 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Feedback Rate (req/s)",
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "expr": "sum(rate(feedback_requests_total{job=~\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "feedback/s",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 16, "y": 0, "w": 8, "h": 6 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" } ] }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Prediction Latency (p50/p95/p99) ms",
      "id": 4,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": { "unit": "ms" },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(prediction_latency_ms_bucket{job=~\"$job\", instance=~\"$instance\"}[5m])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(prediction_latency_ms_bucket{job=~\"$job\", instance=~\"$instance\"}[5m])))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(prediction_latency_ms_bucket{job=~\"$job\", instance=~\"$instance\"}[5m])))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "gridPos": { "x": 0, "y": 6, "w": 24, "h": 9 }
    },
    {
      "type": "timeseries",
      "title": "Memory Usage (MB)",
      "id": 5,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "process_resident_memory_bytes{job=~\"$job\", instance=~\"$instance\"} / 1024 / 1024",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "mebibytes" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single", "sort": "none" } },
      "gridPos": { "x": 0, "y": 15, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "CPU Usage (cores)",
      "id": 6,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(process_cpu_seconds_total{job=~\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "cores" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single", "sort": "none" } },
      "gridPos": { "x": 12, "y": 15, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "GC Collections Rate (by generation)",
      "id": 7,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (generation) (rate(python_gc_collections_total{job=~\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "gen {{generation}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "gridPos": { "x": 0, "y": 23, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "GC Objects Collected Rate (by generation)",
      "id": 8,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (generation) (rate(python_gc_objects_collected_total{job=~\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "gen {{generation}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "gridPos": { "x": 12, "y": 23, "w": 12, "h": 8 }
    },
    {
      "type": "gauge",
      "title": "Open File Descriptors (%)",
      "id": 9,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "100 * (process_open_fds{job=~\"$job\", instance=~\"$instance\"} / process_max_fds{job=~\"$job\", instance=~\"$instance\"})",
          "legendFormat": "FD %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent",
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "showThresholdLabels": false, "showThresholdMarkers": true, "reduceOptions": { "calcs": ["lastNotNull"] } },
      "gridPos": { "x": 0, "y": 31, "w": 8, "h": 8 }
    },
    {
      "type": "stat",
      "title": "Process Uptime",
      "id": 10,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "time() - max(process_start_time_seconds{job=~\"$job\", instance=~\"$instance\"})",
          "legendFormat": "uptime",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "gridPos": { "x": 8, "y": 31, "w": 8, "h": 8 }
    },
    {
      "type": "table",
      "title": "Python Runtime Info",
      "id": 11,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "python_info{job=~\"$job\", instance=~\"$instance\"}",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "options": { "showHeader": true },
      "gridPos": { "x": 16, "y": 31, "w": 8, "h": 8 }
    },
    {
      "type": "stat",
      "title": "Model Learn Rate (updates/s)",
      "id": 12,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(model_learn_total{job=~\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "learn/s",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "gridPos": { "x": 0, "y": 39, "w": 8, "h": 6 }
    },
    {
      "type": "stat",
      "title": "Total Predictions (cumulative)",
      "id": 13,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(prediction_requests_total{job=~\"$job\", instance=~\"$instance\"})",
          "legendFormat": "total",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "gridPos": { "x": 8, "y": 39, "w": 8, "h": 6 }
    },
    {
      "type": "stat",
      "title": "Total Feedback (cumulative)",
      "id": 14,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(feedback_requests_total{job=~\"$job\", instance=~\"$instance\"})",
          "legendFormat": "total",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "gridPos": { "x": 16, "y": 39, "w": 8, "h": 6 }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["arf-ids", "api", "python"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "label": "Datasource",
        "refresh": 1
      },
      {
        "name": "job",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(up, job)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "current": { "selected": true, "text": "All", "value": ".*" },
        "label": "job"
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(up{job=~\"$job\"}, instance)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "current": { "selected": true, "text": "All", "value": ".*" },
        "label": "instance"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "ARF IDS API Metrics",
  "uid": "arf-ids-api",
  "version": 1,
  "weekStart": ""
}